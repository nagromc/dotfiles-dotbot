Host github.com
    Hostname ssh.github.com

Host h
    Port 443

Host git s
    Port 4222

Host h s
    RemoteForward /home/morgan/.gnupg/S.gpg-agent /home/morgan/.gnupg/S.gpg-agent.extra
    ExitOnForwardFailure yes

Include ~/.ssh/config.hostnames
